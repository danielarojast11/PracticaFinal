            <ImageView fitHeight="45.0" fitWidth="44.0" pickOnBounds="true" preserveRatio="true" GridPane.columnIndex="0" GridPane.rowIndex="6">
                <Image url="@cofre.png" />
            </ImageView>
            <TextField fx:id="lblCofre" prefWidth="100" GridPane.columnIndex="1" GridPane.rowIndex="6" />

