    <buttonTypes>
        <ButtonType fx:id="btnCerrar" buttonData="CANCEL_CLOSE" text="Cerrar" />
        <ButtonType fx:id="btnAceptar" buttonData="OK_DONE" text="Aceptar" />
    </buttonTypes>
</DialogPane>
